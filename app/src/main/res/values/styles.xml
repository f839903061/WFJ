<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
    </style>

    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <!--navigation button style-->
    <style name="nav_btn_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:scaleX">0.75</item>
        <item name="android:scaleY">0.75</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/nav_background</item>
        <item name="android:paddingTop">@dimen/nav_paddingTop</item>
        <item name="android:layout_marginLeft">@dimen/nav_paddingLeft</item>
        <item name="android:layout_marginRight">@dimen/nav_paddingRight</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">@string/nav_btn_style_weight_value</item>
    </style>
    <!--personal settings button style-->
    <style name="personal_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@color/personal_title_background</item>
    </style>

    <!--personal concern goods shop browsing list-->
    <style name="personal_concern_list_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:text">0</item>
        <item name="android:textColor">@color/personal_concern_textcolor</item>
    </style>

    <!--personal concern layout-->
    <style name="concern_layout_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <!--<item name="android:background">#88323232</item>-->
        <item name="android:background">#50717171</item>
    </style>

    <style name="common_layout_title">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@color/common_title_text_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="concern_list_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/personal_concern_textcolor</item>
    </style>

    <style name="wait_layout_weight_style">
        <item name="android:layout_width">0.0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1.0</item>

        <item name="android:paddingBottom">3dp</item>
    </style>

    <style name="personal_notify_value_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">-7dp</item>
        <item name="android:layout_marginTop">-5dp</item>
        <item name="android:textSize">9sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/personal_icon_order_notify_bg</item>
        <item name="android:singleLine">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="personal_wait_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2.0dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_centerHorizontal">true</item>

    </style>

    <style name="personal_wait_image_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginTop">10.0dp</item>
    </style>

    <style name="personal_wallet_value_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="more_native_setting_title_style">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/fill_order_delivery_dark</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="more_native_setting_right_tv_style" parent="@style/more_native_setting_title_style">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">@color/fill_order_notice_color</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="more_native_setting_layout_style">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/personal_view_pressed_selector</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">49.0dip</item>
    </style>

    <style name="more_native_setting_arrow_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">13.0dip</item>
        <item name="android:background">@drawable/button_k</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:contentDescription">@string/none</item>
    </style>

    <style name="camera_light_checked" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:button">@drawable/light_click</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="line6">
        <item name="android:background">#ffe1e2e4</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>

    <style name="my_personal_list_height">
        <item name="android:layout_height">49.0dip</item>
    </style>

    <style name="commodity_radiobutton_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/commodity_radiobtn_tv_bg</item>
    </style>

    <style name="fliter_layout_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="gap_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">15.0dip</item>
        <item name="android:background">@color/my_personal_bg</item>
    </style>

    <style name="gap_transparent_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">15.0dip</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="feedback_et_style">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="concern_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="order_item_sdv_style">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="register_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="register_tv_start">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:text">*</item>
    </style>

    <style name="register_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>
</resources>
