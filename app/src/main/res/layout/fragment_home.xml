<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <include android:id="@id/layout_title_search" layout="@layout/home_head_search" />
    <include android:id="@id/layout_title_back" layout="@layout/common_title2" android:visibility="gone"/>
    <FrameLayout
        android:id="@id/home_webView_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="hy.zc.wfj.fragment.HomeFragment">


        <WebView
            android:id="@id/home_webView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center" />
    </FrameLayout>

</LinearLayout>